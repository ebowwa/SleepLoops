  "recommendedSleepCycles": {
    "title": {
      "index": 2,
      "value": "Recommended Sleep Cycles"
    },
    "intro": {
      "index": 3,
      "value": "SleepLoops recommends planning your sleep in multiples of 90-minute cycles."
    },
    "items": [
      {
        "index": 4,
        "value": "‚Ä¢ 6 cycles (9 hours): Optimal for recovery and performance enhancement"
      },
      {
        "index": 5,
        "value": "‚Ä¢ 5 cycles (7.5 hours): Ideal for most adults to feel fully rested"
      },
      {
        "index": 6,
        "value": "‚Ä¢ 4 cycles (6 hours): Minimum recommended for occasional use when time is limited"
      },
      {
        "index": 7,
        "value": "‚Ä¢ 3 cycles (4.5 hours): Emergency option only - not sustainable long-term"
      }
    ]
  },
  "whySleepCyclesMatter": {
    "title": {
      "index": 8,
      "value": "Why Sleep Cycles Matter"
    },
    "items": [
      {
        "index": 9,
        "value": "‚Ä¢ Waking up between cycles rather than during a cycle helps you feel more refreshed."
      },
      {
        "index": 10,
        "value": "‚Ä¢ Each 90-minute cycle includes both REM and non-REM phases essential for mental and physical restoration."
      },
      {
        "index": 11,
        "value": "‚Ä¢ Consistently getting 4-6 complete cycles improves cognitive function, memory, and immune system function."
      },
      {
        "index": 12,
        "value": "‚Ä¢ The app adds a 15-minute buffer to account for the average time it takes to fall asleep."
      },
      {
        "index": 13,
        "value": "‚Ä¢ Aligning with your natural circadian rhythm enhances sleep quality and cycle effectiveness."
      }
    ]
  },
  "sleepPhasesExplained": {
    "title": {
      "index": 14,
      "value": "Sleep Phases Explained"
    },
    "sections": [
      {
        "subtitle": {
          "index": 15,
          "value": "REM Sleep (Rapid Eye Movement)"
        },
        "items": [
          {
            "index": 16,
            "value": "‚Ä¢ Brain is highly active, similar to when awake"
          },
          {
            "index": 17,
            "value": "‚Ä¢ Dreams primarily occur during this phase"
          },
          {
            "index": 18,
            "value": "‚Ä¢ Critical for memory consolidation and learning"
          },
          {
            "index": 19,
            "value": "‚Ä¢ Typically occurs more in later sleep cycles"
          }
        ]
      },
      {
        "subtitle": {
          "index": 20,
          "value": "Non-REM Sleep"
        },
        "items": [
          {
            "index": 21,
            "value": "‚Ä¢ Stage 1: Light sleep, easily awakened"
          },
          {
            "index": 22,
            "value": "‚Ä¢ Stage 2: Body temperature drops, heart rate slows"
          },
          {
            "index": 23,
            "value": "‚Ä¢ Stages 3-4: Deep sleep, most restorative for the body"
          },
          {
            "index": 24,
            "value": "‚Ä¢ Physical healing, immune system strengthening, and tissue repair occur"
          }
        ]
      }
    ]
  },
  "ninetyMinuteCycleBreakdown": {
    "title": {
      "index": 25,
      "value": "90-Minute Cycle Breakdown"
    },
    "items": [
      {
        "index": 26,
        "value": "‚Ä¢ 0-20 min: Light sleep (Stage 1-2)"
      },
      {
        "index": 27,
        "value": "‚Ä¢ 20-70 min: Deep sleep (Stages 3-4)"
      },
      {
        "index": 28,
        "value": "‚Ä¢ 70-90 min: REM sleep"
      },
      {
        "index": 29,
        "value": "‚Ä¢ First cycles of the night contain more deep sleep"
      },
      {
        "index": 30,
        "value": "‚Ä¢ Later cycles contain more REM sleep"
      }
    ]
  },
  "nappingStrategy": {
    "title": {
      "index": 31,
      "value": "Napping Strategy"
    },
    "items": [
      {
        "index": 32,
        "value": "‚Ä¢ Power nap (20 minutes): Boosts alertness and energy without entering deep sleep."
      },
      {
        "index": 33,
        "value": "‚Ä¢ Full cycle nap (90 minutes): Includes all sleep stages and provides more complete rest."
      },
      {
        "index": 34,
        "value": "‚Ä¢ Avoid 30-60 minute naps as waking during deep sleep can cause grogginess (sleep inertia)."
      },
      {
        "index": 35,
        "value": "‚Ä¢ Schedule naps before 3pm to avoid disrupting nighttime sleep."
      }
    ]
  },
